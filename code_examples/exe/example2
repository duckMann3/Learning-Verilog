v0x5aecbae9e1d0_0 .var "y", 7 0;
E_0x5aecbae89650 .event edge, v0x5aecbae9e0e0_0, v0x5aecbae9e000_0, v0x5aecbae88ac0_0;
    .scope S_0x5aecbae4df40;
T_0 ;
    %wait E_0x5aecbae89650;
    %load/vec4 v0x5aecbae9e0e0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 4;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 4;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 4;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 4;
