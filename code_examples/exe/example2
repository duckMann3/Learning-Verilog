    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 4;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %dup/vec4;
    %pushi/vec4 6, 0, 4;
    %cmp/u;
    %jmp/1 T_0.6, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 4;
    %cmp/u;
    %jmp/1 T_0.7, 6;
    %jmp T_0.8;
T_0.0 ;
    %load/vec4 v0x5aecbae88ac0_0;
    %pad/u 8;
    %load/vec4 v0x5aecbae9e000_0;
    %pad/u 8;
    %add;
    %store/vec4 v0x5aecbae9e1d0_0, 0, 8;
    %jmp T_0.8;
T_0.1 ;
    %load/vec4 v0x5aecbae88ac0_0;
    %pad/u 8;
    %load/vec4 v0x5aecbae9e000_0;
    %pad/u 8;
    %sub;
    %store/vec4 v0x5aecbae9e1d0_0, 0, 8;
    %jmp T_0.8;
T_0.2 ;
    %load/vec4 v0x5aecbae88ac0_0;
    %pad/u 8;
    %load/vec4 v0x5aecbae9e000_0;
    %pad/u 8;
    %and;
    %store/vec4 v0x5aecbae9e1d0_0, 0, 8;
    %jmp T_0.8;
T_0.3 ;
    %load/vec4 v0x5aecbae88ac0_0;
    %pad/u 8;
    %load/vec4 v0x5aecbae9e000_0;
    %pad/u 8;
    %or;
    %store/vec4 v0x5aecbae9e1d0_0, 0, 8;
    %jmp T_0.8;
T_0.4 ;
    %pushi/vec4 15, 0, 8;
    %load/vec4 v0x5aecbae88ac0_0;
    %pad/u 8;
    %xor;
    %store/vec4 v0x5aecbae9e1d0_0, 0, 8;
    %jmp T_0.8;
T_0.5 ;
    %pushi/vec4 15, 0, 8;
    %load/vec4 v0x5aecbae88ac0_0;
    %pad/u 8;
    %xor;
    %addi 1, 0, 8;
    %store/vec4 v0x5aecbae9e1d0_0, 0, 8;
    %jmp T_0.8;
T_0.6 ;
    %load/vec4 v0x5aecbae88ac0_0;
    %pad/u 8;
    %load/vec4 v0x5aecbae9e000_0;
    %pad/u 8;
    %mul;
    %store/vec4 v0x5aecbae9e1d0_0, 0, 8;
    %jmp T_0.8;
T_0.7 ;
    %load/vec4 v0x5aecbae88ac0_0;
    %pad/u 8;
    %store/vec4 v0x5aecbae9e1d0_0, 0, 8;
    %load/vec4 v0x5aecbae9e1d0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %store/vec4 v0x5aecbae9e1d0_0, 0, 8;
    %jmp T_0.8;
T_0.8 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5aecbae4dd90;
T_1 ;
    %vpi_func 2 10 "$value$plusargs" 32, "a=%d", v0x5aecbae9e360_0 {0 0 0};
    %nor/r;
    %flag_set/vec4 8;
