 .timescale 0 0;
v0x5aecbae9e360_0 .var "a", 3 0;
v0x5aecbae9e440_0 .var "b", 3 0;
v0x5aecbae9e510_0 .var "s", 3 0;
v0x5aecbae9e610_0 .net "y", 7 0, v0x5aecbae9e1d0_0;  1 drivers
E_0x5aecbae89dc0 .event edge, v0x5aecbae9e1d0_0;
S_0x5aecbae4df40 .scope module, "a1" "alu" 2 7, 3 1 0, S_0x5aecbae4dd90;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /INPUT 4 "b";
    .port_info 2 /INPUT 4 "s";
    .port_info 3 /OUTPUT 8 "y";
v0x5aecbae88ac0_0 .net "a", 3 0, v0x5aecbae9e360_0;  1 drivers
v0x5aecbae9e000_0 .net "b", 3 0, v0x5aecbae9e440_0;  1 drivers
